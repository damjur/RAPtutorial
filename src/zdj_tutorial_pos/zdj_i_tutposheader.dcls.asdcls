@EndUserText.label: 'Header Authorisation'
@MappingRole: true
define role ZDJ_I_TUTPOSHEADER {
    grant 
        select
            on
                ZDJ_I_TUTPOSHEADER
                    where
                        ( ) = aspect  pfcg_auth(S_TABU_NAM, 
                                TABLE = 'ZDJ_TUT_POS_HEAD' , 
                                ACTVT = '03' );
                        
}